<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="e.sundaraganapathyl.kssolutions">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Authentication"
            android:label="Authentication"></activity>
        <activity
            android:name=".Assignment"
            android:label="Work Assignment" />
        <activity
            android:name=".DayWiseActivity"
            android:label="Activity" />
        <activity
            android:name=".DateWise_Activity"
            android:label="Logs" />
        <activity
            android:name=".registeration"
            android:label="Registration" />
        <activity
            android:name=".MainActivity"
            android:label="Notify">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>